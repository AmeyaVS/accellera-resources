struct testT { const char* name; int i; int j;};
static testT (*testP)[];
extern testT test[];
int main() {
  testP=(testT(*)[])(&test);
  static char str[10];
  int i=0, failed=0;
  while((*testP)[i].name){
    sprintf(str, "test%d",i);
    if(strcmp(str, (*testP)[i].name) != 0) ++failed;
    i++;
  }
  if(failed) { exit (1); }
