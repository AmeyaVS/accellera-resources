  src/cudd/2.3.0/cudd/doc/Makefile \
  src/cudd/2.3.0/cudd/doc/icons/Makefile \
  src/cudd/2.3.0/dddmp/Makefile \
  src/cudd/2.3.0/dddmp/doc/Makefile \
